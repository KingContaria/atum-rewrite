  "authors": [
    "Void_X_Walker"
  ],
  "license": "All-Rights-Reserved",
  "icon": "assets/atum/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.voidxwalker.autoreset.Atum"
    ]
