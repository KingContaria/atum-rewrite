{
  "schemaVersion": 1,
  "id": "atum",
  "version": "${version}",
  "name": "Atum",
  "description": "Mod to automatically reset Random and Set Seeds",
  "authors": [
    "Void_X_Walker"
  ],
  "license": "MIT",
  "icon": "assets/atum/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.voidxwalker.autoreset.Atum"
    ]
  },
  "mixins": [
    "autoreset.mixins.json"
  ],
  "accessWidener": "atum.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.0",
    "speedrunapi": "*"
  },
  "breaks": {
    "fast_reset": "<2.0-alpha.1",
    "worldpreview": "<6.0.0-alpha.1"
  },
  "custom": {
    "speedrunapi": {
      "config": "me.voidxwalker.autoreset.AtumConfig"
    }
  }
}
